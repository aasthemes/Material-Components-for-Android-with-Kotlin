<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
>
    <Button
        android:id="@+id/menu_17_btn_popup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAB Popup"
        android:padding="8dp"
    />
    <Button
        android:id="@+id/menu_17_btn_icon_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAB Icon Label"
        android:padding="8dp"
    />
    <Button
        android:id="@+id/menu_17_btn_stroke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAB Stroke Loading"
        android:padding="8dp"
    />
    <Button
        android:id="@+id/menu_17_btn_position"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FAB Custom Position"
        android:padding="8dp"
    />
</LinearLayout>